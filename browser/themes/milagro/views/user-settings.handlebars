			<div class="mpinSettingsRow mpVerCenter" title="{{ data.name }}" alt="{{ data.name }}">
				{{ data.name }}
			</div>
		</li>
	</ul>
</div>

<div class="mpinMain">
	<div class="mpinBtnBox">
		<div class="mpinButton mpinBtnClick" id="mpin_cancel_btn">
			{{txt "account_button_backToList2"}}
