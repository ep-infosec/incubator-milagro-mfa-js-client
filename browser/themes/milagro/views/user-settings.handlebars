		<div class="mpinButton mpinBtnClick" id="mpin_reactivate_btn">
			{{txt "account_button_reactivate2"}}
		</div>
		
		<div class="mpinButton mpinWarnBtn mpinBtnClick" id="mpin_deluser_btn">
			{{txt "account_button_delete2"}}
		</div>
